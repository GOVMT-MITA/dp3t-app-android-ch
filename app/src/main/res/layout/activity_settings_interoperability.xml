<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/interoperability_toolbar"
        app:title="@string/interop_mode_title_bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationContentDescription="@string/android_accessibility_back_button"
        app:navigationIcon="@drawable/ic_arrow_back" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/settings_interoperability_fragment_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/interoperability_toolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>